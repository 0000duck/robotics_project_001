            </collision>
            <visual name="drive_wheel_left_visual">
                <pose>0.074873 0.34278 0.0867060 0 0</pose>
                <geometry>
                    <mesh>
                        <uri>model://robot/meshes/drive_wheel_left.STL</uri>
                        <!-- <scale>0.001 0.001 0.001</scale> -->
                    </mesh>
