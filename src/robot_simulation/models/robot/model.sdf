        <joint name="drive_wheel_right_joint" type="revolute">
            <parent>base_footprint</parent>
            <child>drive_wheel_right</child>
            <pose>0.074873 -0.34278 0.086706 0.0 0.0 0.0</pose>
            <axis>
                <xyz>0 1 0</xyz>
            </axis>
        </joint>

        <link name="drive_wheel_left">
