                <inertia>
                    <ixx>1.07</ixx>
                    <ixy>0</ixy>
                    <ixz>0.09</ixz>
                    <iyy>4.55</iyy>
                    <iyz>0</iyz>
                    <izz>5.30</izz>
                </inertia>
                <mass>49.497</mass>
            </inertial>
            <collision name="base_link_collision">
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                    <mesh>
                        <uri>model://robot/meshes/chassis.STL</uri>
                        <!-- <scale>0.001 0.001 0.001</scale> -->
